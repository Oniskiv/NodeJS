{
  "name": "epam-nodejs-training",
  "version": "0.0.3",
  "description": "",
  "main": "out/app.js",
  "scripts": {
    "start": "nodemon --exec npm run build-start",
    "build-start": "npm run build && node out/index.js",
    "build": "babel src -d out --plugins=babel-plugin-transform-es2015-modules-commonjs",
    "test": "",
    "start-help": "npm run build && node out/app.js -h",
    "start-reverse": "npm run build && node out/app.js --action=reverse",
    "start-transform": "npm run build && node out/app.js -a transform",
    "start-outputFile": "npm run build && node out/app.js --action=outputFile --file=./data/products.csv",
    "start-convertFromFile": "npm run build && node out/app.js --action=convertFromFile -f ./data/products.csv",
    "start-convertToFile": "npm run build && node out/app.js -a convertToFile --file=./data/products.csv",
    "start-cssBundler": "npm run build && node out/app.js --action=cssBundler --path=./data/css/"
  },
  "author": "Oniskiv Vladislav <onisckif.vladislaw@mail.ru>",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "colors": "^1.3.0",
    "commander": "^2.15.1",
    "cookie-parser": "^1.4.3",
    "csv2json": "^1.4.2",
    "csvjson": "^5.0.0",
    "csvtojson": "^1.1.9",
    "express": "^4.16.3",
    "nodemon": "^1.17.3",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0"
  }
}
